{
	"expo": {
		"name": "Meetdev",
		"slug": "Meetdev",
		"version": "1.0.0",
		"scheme": "meetdev",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.meetdev.meetdev",
			"config": {
				"usesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.meetdev.meetdev"
		},
		"web": {
			"favicon": "./assets/images/logo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you add a photo to your profile.",
					"microphonePermission": false,
					"cameraPermission": false
				}
			],
			"@react-native-google-signin/google-signin"
		],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "acc5558d-0f48-4d6c-8c27-09374de3d6f1"
			}
		}
	}
}
